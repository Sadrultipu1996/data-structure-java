public class Task06{
  
  public static void palindromeChecker(int [] a){
    boolean flag= true;
    for(int i=0,j=a.length-1; i<j; i++,j--){
      if(a[i]!=a[j]){
        flag=false;
        break;
      }
    }
    if(flag){
      System.out.println("Palindrome");
    }else{
      System.out.println("Not Palindrome");
    }
  } 
  
  public static void main(String[]args){
    int [] a= {4,2,6,2,4};
    palindromeChecker(a);
  }
}